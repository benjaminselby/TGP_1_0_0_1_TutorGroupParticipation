    <!-- SQL QUERIES -->
    <add key="userInfoQry" value="
          SELECT ID, Title, Preferred, Surname, NameExternal, OccupEmail 
          FROM Community 
          WHERE NetworkLogin = @CurrentUserNetworkLogin" />
    
